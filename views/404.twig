{% extends 'base.twig' %}
{% block styles %}
	<link rel="stylesheet" href="/assets/bootstrap/css/bootstrap.css">
	<link rel="stylesheet" href="/assets/bootstrap/css/bootstrap-theme.css">
	<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Aclonica" />
	<link rel="stylesheet" href="/assets/styles/main.css">
{% endblock %}
{% block title %}Not found{% endblock %}
{% block header %}
	{% include '/parts/header.twig' %}
{% endblock %}
{% block content %}
	<div class="row">
		<div class="col-md-12 not-found">
			<div class="row">
				<div class="col-md-12 text-center error">404</div>
			</div>
			<div class="row">
				<div class="col-md-12 text-center">
					<p class="styled">
						Oh no!<br>
						The page <ins>'{{ page }}'</ins> does not exist!
					</p>
					<p>
						Perhaps you should start from <a href="/" title="Home page">here</a>.
					</p>
				</div>
			</div>
		</div>
	</div>
{% endblock %}
{% block footer %}
	{% include '/parts/footer.twig' %}
{% endblock %}
